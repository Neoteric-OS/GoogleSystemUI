<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/indicator_solid">
        <shape android:shape="rectangle">
            <solid android:color="?android:^attr-private/materialColorPrimaryContainer"/>
            <corners android:radius="28dp"/>
        </shape>
    </item>
    <item android:id="@+id/indicator_stroke">
        <shape android:shape="rectangle">
            <corners android:radius="28dp"/>
            <stroke
                android:width="1dp"
                android:color="?android:^attr-private/materialColorPrimaryContainer"/>
        </shape>
    </item>
</layer-list>
