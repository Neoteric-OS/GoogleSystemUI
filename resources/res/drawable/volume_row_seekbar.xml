<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" android:paddingMode="stack">
    <item
        android:gravity="center"
        android:id="@android:id/background">
        <shape>
            <size android:height="@dimen/volume_dialog_track_width"/>
            <corners android:radius="@dimen/volume_dialog_panel_width_half"/>
            <solid android:color="?android:^attr-private/materialColorOutlineVariant"/>
        </shape>
    </item>
    <item
        android:gravity="center"
        android:id="@android:id/progress">
        <com.android.systemui.util.RoundedCornerProgressDrawable android:drawable="@drawable/volume_row_seekbar_progress"/>
    </item>
</layer-list>
